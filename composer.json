{
    "name": "my/demo-project",
    "description": "A demo PHP project for generating a dependency tree",
    "type": "project",
    "license": "MIT",
    "require": {
         "phpmailer/phpmailer": "5.2.21",
        "monolog/monolog": "1.1.0",
        "firebase/php-jwt": "2.2.0",
        "illuminate/database": "5.4.0",
        "symfony/http-foundation": "2.7.0",
        "twig/twig": "1.18.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "DemoProject\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
